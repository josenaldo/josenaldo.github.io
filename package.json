    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"contentlayer2 dev\" \"next dev -p 3500\"",
        "build": "contentlayer2 build && next build",
        "postbuild": "next-sitemap",
        "start": "next start",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "format:all": "yarn format && yarn lint:fix",
        "info": "next info",
        "lighthouse": "lighthouse http://localhost:6500 --view --chrome-flags='--headless' --output json --output-path .tmp/lighthouse-report.json"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mdx-js/react": "^3.1.0",
        "@mui/icons-material": "^7.3.8",
        "@mui/lab": "^7.0.1-beta.22",
        "@mui/material": "^7.3.8",
        "axios": "^1.8.3",
        "concurrently": "^9.1.2",
        "contentlayer2": "^0.5.8",
        "date-fns": "4.1.0",
        "date-fns-tz": "^3.2.0",
        "eslint": "^9.22.0",
        "eslint-config-next": "^16.1.6",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-react": "^7.37.4",
        "next": "^16.1.6",
        "next-contentlayer2": "^0.5.8",
        "next-seo": "^7.2.0",
        "next-sitemap": "^4.2.3",
        "prop-types": "^15.8.1",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-markdown": "^10.1.0",
        "rehype-external-links": "^3.0.0",
        "rehype-prism-plus": "^2.0.0",
        "rehype-raw": "^7.0.0",
        "remark-gfm": "^4.0.1",
        "remark-parse": "^11.0.0"
    },
    "license": "MIT",
    "devDependencies": {
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "lighthouse": "^13.0.3",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3"
    }
